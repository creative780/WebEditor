# Next.js Frontend Environment Variables
# Copy this file to .env.local and update the values as needed

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000

# App Configuration
NEXT_PUBLIC_APP_NAME=Web-to-Print App
NEXT_PUBLIC_APP_VERSION=1.0.0

# Feature Flags (for future use)
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG_TOOLS=false

# External Services (for future use)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
NEXT_PUBLIC_SENTRY_DSN=

